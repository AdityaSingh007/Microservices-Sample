# Keycloak identity server host name
Hostname=192.168.31.72

# Keycloak identity server port
Port=8443

# Microservices https certificate password
ASPNETCORE_Kestrel_Certificates_Default_Password=Test#123

# Keycloak admin password
KEYCLOAK_ADMIN_PASSWORD=AAAAAAAA

# Keycloak database password
DB_PASSWORD=password

# Microservices database password
MSSQL_SA_PASSWORD=Appuser#123

# Postgres server login password
POSTGRES_PASSWORD=password

# Keycloak https server port
KEYCLOAK_HTTPS_SERVER_PORT=8443

# Project namespace (defaults to the current folder name if not set)
#COMPOSE_PROJECT_NAME=myproject


# Password for the 'elastic' user (at least 6 characters)
ELASTIC_PASSWORD=Aditya#123


# Password for the 'kibana_system' user (at least 6 characters)
KIBANA_PASSWORD=Aditya#123


# Version of Elastic products
STACK_VERSION=8.7.1


# Set the cluster name
CLUSTER_NAME=docker-cluster


# Set to 'basic' or 'trial' to automatically start the 30-day trial
LICENSE=basic
#LICENSE=trial


# Port to expose Elasticsearch HTTP API to the host
ES_PORT=9200


# Port to expose Kibana to the host
KIBANA_PORT=5601


# Increase or decrease based on the available host memory (in bytes)
