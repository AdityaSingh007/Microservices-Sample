{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AuthenticationParameters": {
    "Audience": [ "Microservice1", "gatewaytodownstreamtokenexchangeclient" ],
    "Authority": "http://microservices-keycloak-identity:8080/auth/realms/MicroserviceIdentity",
    "Scope": "microservice1_fullaccess"
  },
  "ElasticUrl": "http://host.docker.internal:9200",
  "ConnectionStrings": {
    "MessagingDb": "Server=microservices-sqlserver,1433;database=microservices_db;TrustServerCertificate=True",
    "CustomerConnectionString": "Server=microservices-sqlserver,1433;database=customer_db;TrustServerCertificate=True"
  },
  "AllowedHosts": "*"
}
